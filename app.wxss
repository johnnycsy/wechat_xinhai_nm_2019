/**app.wxss**/
@import "./utils/fonts/iconfont.wxss";

page {
  overflow-x: hidden;
  overflow-y: scroll !important;
}

.main {
  overflow-x: hidden;
  overflow-y: auto;
}

.icon-12 {
  font-size: 1.2rem;
}

.icon-14 {
  font-size: 1.4rem;
}

.icon-16 {
  font-size: 1.6rem;
}

.icon-18 {
  font-size: 1.8rem;
}

.icon-20 {
  font-size: 2rem;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
