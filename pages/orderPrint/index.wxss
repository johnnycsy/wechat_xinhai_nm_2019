/* pages/orderPrint/index.wxss */

.header {
  display: table;
  width: 100%;
  margin: 25% auto;
  text-align: center;
}

.header-but {
  display: table-cell;
  text-align: center;
}

.header-but >text {
  display: inline-block;
  padding: .5rem 1rem;
  background: #5071BA;
  border-radius: 2rem;
  color: white;
}

.other-info{
  display: flex;
  justify-content: center;
  align-content: center;
  padding: 1rem;
  text-align: center;
}