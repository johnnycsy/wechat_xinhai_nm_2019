/* pages/maps/index.wxss */

map {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  z-index: 0;
}

.getMyMaps {
  border-radius: 100%;
  width: 2rem;
  height: 2rem;
  line-height: 2rem;
  display: flex;
  justify-content: center;
  align-content: center;
  z-index: 9;
  position: fixed;
  left: 0.5rem;
  top: 0.5rem;
  font-size: 1rem;
}
